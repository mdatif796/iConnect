<h1>page header</h1>

<% if(locals.user){ %>
    <ul>
        <li>
            <h1><a href="/users/profile"><%= user.name %> </a> </h1>
        </li>
        <li>
            <a href="/users/sign-out">Sign Out</a>
        </li>
    </ul>
    <% }else{ %>
        <ul>
            <li>
                <a href="/users/sign-in">Sign In</a>
            </li>
            <li>
                <a href="/users/sign-up">Sign Up</a>
            </li>
        </ul>    
<% } %> 